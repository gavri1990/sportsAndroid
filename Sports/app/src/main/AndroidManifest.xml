<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sports">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/sports"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sports"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Basketball"></activity>
        <activity android:name=".Tracker" />
        <activity android:name=".Menu"></activity>
        <activity android:name=".Football" />
        <activity android:name=".Volleyball" />
        <activity android:name=".Tennis" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>